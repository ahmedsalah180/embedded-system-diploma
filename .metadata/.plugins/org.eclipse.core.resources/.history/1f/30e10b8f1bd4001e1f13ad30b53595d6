#include <stdio.h>
#include <stdlib.h>

int main()
{
	int n, arr[100];
	printf("enter the number of elements:");
	fflush(stdout);
	scanf("%d",&n);
	for(int i=0;i<n;i++)
	{
		printf("%d\t",arr[i+1]);
	}
	/*for(int i=0;i<n;i++)
	{
		printf("%d\t",arr[i+1]);
	}*/

}
