#include <stdio.h>
#include <stdlib.h>

int main()
{
	int arrsize;
	printf("enter the number of elements:");
	fflush(stdout);
	scanf("%d",&arrsize);
	for(int i=0;i<arrsize;i++)
		printf("%d\t",i+1);
}
